{
    "minecraft:boss": {
        "should_darken_sky": "$general.boolean",
        "hud_range": "$general.number"
    },
    "minecraft:npc": {
        "npc_data": {
            "portrait_offsets": {
                "translate": {
                    "$dynamic.list.index_triple": "$general.number"
                },
                "scale": {
                    "$dynamic.list.index_triple": "$general.number"
                }
            },
            "picker_offsets": {
                "translate": {
                    "$dynamic.list.index_triple": "$general.number"
                },
                "scale": {
                    "$dynamic.list.index_triple": "$general.number"
                }
            },
            "skin_list": {
                "$dynamic.list.next_index": {
                    "variant": "$general.number"
                }
            }
        }
    },
    "minecraft:water_movement": {
        "drag_factor": "$general.decimal"
    }
}
{
    "minecraft:entity": {
        "format_version": "$general.format_version",
        "do_not_upgrade": {},
        "description": {
            "identifier": "$general.entity_identifier",
            "runtime_identifier": "$general.entity_identifier",
            "is_spawnable": "$general.boolean",
            "is_summonable": "$general.boolean",
            "is_experimental": "$general.boolean"
        },
        "component_groups": {
            "$placeholder": "$entity_components and $entity_behavior and $entity_annotations"
        },
        "components": "$entity_components and $entity_behavior and $entity_annotations",
        "events": {
            "$placeholder": {
                "sequence": {
                    "$dynamic.list.next_index": {
                        "filters": "$entity_filters",
                        "$load": "$entity.minecraft:entity.events.$placeholder"
                    }
                },
                "randomize": {
                    "$dynamic.list.next_index": {
                        "weight": "$general.number",
                        "$load": "$entity.minecraft:entity.events.$placeholder"
                    }
                },
                "add": {
                    "component_groups": {
                        "$dynamic.list.next_index": "$dynamic.entity.component_groups"
                    }
                },
                "remove": "$entity.minecraft:entity.events.$placeholder.add"
            },
            "minecraft:entity_born": "$entity.minecraft:entity.events.$placeholder",
            "minecraft:entity_spawned": "$entity.minecraft:entity.events.$placeholder",
            "minecraft:entity_transformed": "$entity.minecraft:entity.events.$placeholder",
            "minecraft:on_prime": "$entity.minecraft:entity.events.$placeholder"
        }
    }
}
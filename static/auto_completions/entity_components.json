{
    "minecraft:addrider": {
        "entity_type": "$general.entity_identifier"
    },
    "minecraft:ageable": {
        "duration": "$general.number",
        "feedItems": {
            "$dynamic.next_list_index": "$general.item_identifier"
        },
        "grow_up": "$general.entity.event_definition"
    },
    "minecraft:attack": {
        "damage": "$general.number",
        "effect_name": "$general.effect_name",
        "effect_duration": "$general.number"
    },
    "minecraft:breathable": {
        "breatheBlocks": {
            "$dynamic.next_list_index": "$general.block_identifier"
        },
        "breathesWater": "$general.boolean",
        "breathesLava": "$general.boolean",
        "breathesAir": "$general.boolean",
        "breathesSolids": "$general.boolean",
        "generatesBubbles": "$general.boolean",
        "inhaleTime": "$general.decimal",
        "suffocateTime": "$general.number",
        "totalSupply": "$general.number",
        "nonBreatheBlocks": {
            "$dynamic.next_list_index": "$general.block_identifier"
        }
    },
    "minecraft:can_climb": {},
    "minecraft:can_fly": {},
    "minecraft:collision_box": {
        "width": "$general.number",
        "height": "$general.number"
    },
    "minecraft:fall_damage": {
        "value": "$general.decimal"
    },
    "minecraft:fire_immune": "$general.boolean",
    "minecraft:follow_range": {
        "value": "$general.number",
        "max": "$general.number"
    },
    "minecraft:health": {
        "value": "$general.number",
        "max": "$general.number",
        "min": "$general.number"
    },
    "minecraft:hurt_when_wet": {
        "value": "$general.boolean"
    },
    "minecraft:is_stackable": {
    },
    "minecraft:is_baby": {
    },
    "minecraft:jump.dynamic": {},
    "minecraft:jump.static": {
        "jump_power": "$general.decimal"
    },
    "minecraft:leashable": {
        "soft_distance": "$general.decimal",
        "hard_distance": "$general.decimal",
        "max_distance": "$general.decimal",
        "on_leash": "$general.entity.event_definition",
        "on_unleash": "$general.entity.event_definition"
    },
    "minecraft:loot": {
        "table": "$dynamic.loot_table_files"
    },
    "minecraft:movement": {
        "value": "$general.decimal"
    },

    "minecraft:movement.amphibious": {
        "max_turn": "$general.degrees"
    },
    "minecraft:movement.basic": "$entity_components.minecraft:movement&dot;amphibious",
    "minecraft:movement.fly": "$entity_components.minecraft:movement&dot;amphibious",
    "minecraft:movement.generic": "$entity_components.minecraft:movement&dot;amphibious",
    "minecraft:movement.jump": "$entity_components.minecraft:movement&dot;amphibious",
    "minecraft:movement.skip": "$entity_components.minecraft:movement&dot;amphibious",
    "minecraft:movement.sway": "$entity_components.minecraft:movement&dot;amphibious",

    "minecraft:nameable": {
        "allowNameTagRenaming": "$general.boolean",
        "alwaysShow": "$general.boolean",
        "default_trigger": "$general.entity.event_definition",
        "name_actions": {
            "$dynamic.next_list_index": {
                "name_filter": {},
                "on_named": "$general.entity.event_definition"
            }
        }
    },

    "minecraft:navigation.climb": {
        "avoid_portals": "$general.boolean",
        "avoid_sun": "$general.boolean",
        "avoid_water": "$general.boolean",
        "can_open_doors": "$general.boolean",
        "can_pass_doors": "$general.boolean",
        "can_path_over_water": "$general.boolean",
        "can_sink": "$general.boolean"
    },
    "minecraft:navigation.float": "$entity_components.minecraft:navigation&dot;climb",
    "minecraft:navigation.fly": "$entity_components.minecraft:navigation&dot;climb",
    "minecraft:navigation.generic": "$entity_components.minecraft:navigation&dot;climb",
    "minecraft:navigation.swim": "$entity_components.minecraft:navigation&dot;climb",
    "minecraft:navigation.walk": "$entity_components.minecraft:navigation&dot;climb",

    "minecraft:physics": {
        "has_collision": "$general.boolean",
        "has_gravity": "$general.boolean"
    },
    "minecraft:projectile": {
        "onHit": {
            "impact_damage": {
                "damage": {
                    "$dynamic.next_list_index": "$general.number"
                },
                "knockback": "$general.boolean",
                "semirandomdiffdamage": "$general.boolean",
                "destroyonhit": "$general.boolean",
                "maxcriticaldamage": "$general.number",
                "mincriticaldamage": "$general.number",
                "powermultiplier": "$general.decimal"
            },
            "stick_in_ground": {
                "shake_time" : "$general.decimal"
            },
            "arrow_effect": {
            }
        },
        "hitSound": "$general.sound",
        "power": "$general.decimal",
        "gravity": "$general.decimal",
        "uncertaintyBase": "$general.number",
        "uncertaintyMultiplier": "$general.number",
        "anchor": "$general.number",
        "shouldbounce": "$general.boolean",
        "offset": {
            "$dynamic.next_list_index": "$general.decimal"
        }
    },
    "minecraft:rideable": {
        "controlling_seat": "$general.number",
        "crouching_skip_interact": "$general.boolean",
        "family_types": {
            "$dynamic.next_list_index": "$general.family"
        },
        "interact_text": {},
        "pull_in_entities": "$general.boolean",
        "rider_can_interact": "$general.boolean",
        "seat_count": "$general.number",

        "seats": {
            "$dynamic.next_list_index": {
                "lock_rider_rotation": "$general.degrees",
                "max_rider_count": "$general.number",
                "min_rider_count": "$general.number",
                "position": {
                    "$dynamic.next_list_index": "$general.decimal"
                },
                "rotate_rider_by": "$general.degrees"
            }
        }
    },
    "minecraft:scale": {
        "value": "$general.decimal"
    },
    "minecraft:shooter": {
        "type": {},
        "def": "$general.entity_identifier"
    },
    "minecraft:type_family": {
        "family": {
            "$dynamic.next_list_index": "$general.entity.family"
        }
    }
}